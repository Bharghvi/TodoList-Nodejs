<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Todo</title>
    <script src="https://code.jquery.com/jquery-3.2.1.min.js" crossorigin="anonymous"></script>
    <script src="assets/todo-list.js" type="text/javascript">

    </script>
    <link rel="stylesheet" href="/assets/styles.css">
  </head>
  <body>

    <div class="todo-table">
      <h1>Todo list</h1>
      <form class="todo_add" action="index.html" method="post">
        <input type="text" name="item" value="" placeholder="Type a todo">
        <button type="submit" name="button">Add Item</button>
      </form>

      <ul>
        <% for(var i = 0; i < todos.length; i++) { %>
          <li><%= todos[i].item %></li>
          <% } %>
      </ul>
    </div>
  </body>
</html>
